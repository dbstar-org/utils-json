<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>com.yeecloud.commons.utils</groupId>
    <artifactId>UTILS</artifactId>
    <version>1.0.0</version>
  </parent>
  <artifactId>utils-json</artifactId>
  <version>0.0.1-SNAPSHOT</version>

  <properties>
    <!-- json impl -->
    <version.org.json>20180813</version.org.json>
    <version.fastjson>1.2.56</version.fastjson>
    <version.jackson-databind>2.7.9.5</version.jackson-databind>
    <version.gson>2.8.5</version.gson>
    <version.net.sf.json>2.4</version.net.sf.json>
  </properties>

  <dependencies>
    <!-- json impl -->
    <dependency>
      <groupId>org.json</groupId>
      <artifactId>json</artifactId>
      <version>${version.org.json}</version>
      <scope>provided</scope>
      <optional>true</optional>
    </dependency>
    <dependency>
      <groupId>com.alibaba</groupId>
      <artifactId>fastjson</artifactId>
      <version>${version.fastjson}</version>
      <scope>provided</scope>
      <optional>true</optional>
    </dependency>
    <dependency>
      <groupId>com.fasterxml.jackson.core</groupId>
      <artifactId>jackson-databind</artifactId>
      <version>${version.jackson-databind}</version>
      <scope>provided</scope>
      <optional>true</optional>
    </dependency>
    <dependency>
      <groupId>com.google.code.gson</groupId>
      <artifactId>gson</artifactId>
      <version>${version.gson}</version>
      <scope>provided</scope>
      <optional>true</optional>
    </dependency>
    <dependency>
      <groupId>net.sf.json-lib</groupId>
      <artifactId>json-lib</artifactId>
      <version>${version.net.sf.json}</version>
      <classifier>jdk15</classifier>
      <scope>provided</scope>
      <optional>true</optional>
    </dependency>
    <!-- support -->
    <dependency>
      <groupId>com.yeecloud.commons.utils</groupId>
      <artifactId>utils-net-api</artifactId>
      <version>0.0.1-SNAPSHOT</version>
      <scope>provided</scope>
      <optional>true</optional>
    </dependency>
  </dependencies>
</project>
